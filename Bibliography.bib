@InCollection{Feher2017,
  author    = {Joseph Feher},
  booktitle = {Quantitative Human Physiology (Second Edition)},
  publisher = {Academic Press},
  title     = {3.6 - The Neuromuscular Junction and Excitation-Contraction Coupling},
  year      = {2017},
  address   = {Boston},
  edition   = {Second Edition},
  editor    = {Joseph Feher},
  isbn      = {978-0-12-800883-6},
  pages     = {318--333},
  abstract  = {This chapter points out that muscles are slaves to the nerves and that motor neuron activity ultimately controls muscle activity. Motor neurons are activated by the integration of multiple inputs from multiple sources. Stimulatory connections produce EPSPs-excitatory postsynaptic potentials; inhibitory connections produce IPSPs-inhibitory postsynaptic potentials. This chapter discusses the time and space evolution of EPSPs and IPSPs, and describes spatial summation and temporal summation. Neuromuscular transmission is then described. Excitation-contraction coupling involving the sarcolemma, T-tubule, and sarcoplasmic reticulum is then described. The turn-on of muscle contraction through Ca2 transients begun by Ca2 release from the SR through the activation of the ryanodine receptor is discussed. Activation of muscle contraction by Ca2 binding to troponin C and subsequent removal of activator Ca2 by reuptake into the SR mediated by the SERCA pump are discussed.},
