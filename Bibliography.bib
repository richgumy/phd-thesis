@article{Zare2021,
	title        = {{Silicone-based biomaterials for biomedical applications: Antimicrobial strategies and 3D printing technologies}},
	author       = {Mina Zare and Erfan Rezvani Ghomi and Prabhuraj D. Venkatraman and Seeram Ramakrishna},
	year         = 2021,
	month        = oct,
	journal      = {Journal of Applied Polymer Science},
	publisher    = {{John Wiley & Sons, Ltd}},
	volume       = 138,
	pages        = 50969,
	doi          = {10.1002/app.50969},
	issn         = {1097-4628},
	url          = {https://onlinelibrary.wiley.com/doi/full/10.1002/app.50969},
	abstract     = {Silicone is a synthetic polymer widely used in the biomedical industry as implantable devices since 1940, owing to its excellent mechanical properties and biocompatibility. Silicone biomaterials are renowned for their biocompatibility due to their inert nature and hydrophobic surface. A timeline illustration shows critical development periods of using silicone in varied biomedical applications. In this review, silicone properties are discussed along with several biomedical applications, including medical inserts, speciality contact lenses, drains and shunts, urinary catheters, reconstructive gel fillers, craniofacial prosthesis, nerve conduits, and metatarsophalangeal joint implants. Silicones are prone to microbial infections when exposed and interactions with the host tissue. As in the case of medical inserts, the development of specific antimicrobial strategies is essential. The review highlights silicone implants' interaction with soft and bone tissue and various antimicrobial strategies, including surface coating, physical or chemical modifications, treating with antibiotics or plasma-activated surfaces to develop the resistance to bacterial infection. Finally, 3D printing technology, tissue engineering, regenerative medicine applications, and future trends are also critically presented, indicating the silicone's potential as a biomaterial.},
	issue        = 38,
	keywords     = {3D printing,antimicrobial strategies,biomedical applications,silicone,tissue engineering}
}
@article{Klimm2022,
	title        = {{Mechanism of resistance relaxation and hysteresis in viscoelastic piezoresistive polymer nanocomposites}},
	author       = {Wolfgang Klimm and Kawai Kwok},
	year         = 2022,
	month        = aug,
	journal      = {International Journal of Mechanics and Materials in Design 2022},
	publisher    = {{Springer}},
	pages        = {1--15},
	doi          = {10.1007/s10999-022-09603-y},
	isbn         = {0123456789},
	issn         = {1573-8841},
	abstract     = {Carbon nanotube polymer nanocomposites exhibit outstanding mechanical, electrical, and thermal properties, enabling a myriad of sensor and actuation applications. Piezoresistivity in particular is achieved by the creation of a deformation-sensitive electrical conductor network and is the underlying effect of most structural health and motion sensing. The polymer matrix, however, introduces time-dependent mechanical behavior known as viscoelasticity. Viscoelasticity affects the relationship between relative resistance change and mechanical strain over time and becomes particularly important over long times, as well as at high temperatures, radiation aging, and in the presence of high mechanical stresses. The characterization of these influences on the piezoresistivity is therefore crucial to maintain sensor stability under harsh conditions. While experimental studies have been conducted, the effect of viscoelasticity has not been thorougly analyzed. Here we study resistance relaxation and hysteresis in carbon nanotube nanocomposites under the influence of matrix viscoelasticity. First we characterize the multiaxial viscoelastic behavior directly without invoking the common assumption of constant Poisson’s ratio, which is central to predicting resistnace relaxation and hysteresis. We then apply the viscoelastic master curve to a micromechanics-based numerical piezoresistive model and study the relative resistance change under relaxation, creep, and deformation-controlled cyclic loading. The results are compared to the common assumptions of constant Poisson’s ratio and constant bulk modulus. We show that the time-dependent transverse deformation leads to network compaction and is the mechanism behind resistance relaxation and hysteresis.},
	keywords     = {Characterization and Evaluation of Materials,Classical Mechanics,Engineering Design,Solid Mechanics}
}
@article{Su2022,
	title        = {{Optimization of Piezoresistive Strain Sensors Based on Gold Nanoparticle Deposits on PDMS Substrates for Highly Sensitive Human Pulse Sensing}},
