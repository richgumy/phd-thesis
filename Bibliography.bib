   month = {10},
   pages = {50969},
   publisher = {John Wiley & Sons, Ltd},
   title = {Silicone-based biomaterials for biomedical applications: Antimicrobial strategies and 3D printing technologies},
   volume = {138},
   url = {https://onlinelibrary.wiley.com/doi/full/10.1002/app.50969 https://onlinelibrary.wiley.com/doi/abs/10.1002/app.50969 https://onlinelibrary.wiley.com/doi/10.1002/app.50969},
   year = {2021},
