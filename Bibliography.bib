   year = {2022},
}
@article{Klimm2022,
   abstract = {Carbon nanotube polymer nanocomposites exhibit outstanding mechanical, electrical, and thermal properties, enabling a myriad of sensor and actuation applications. Piezoresistivity in particular is achieved by the creation of a deformation-sensitive electrical conductor network and is the underlying effect of most structural health and motion sensing. The polymer matrix, however, introduces time-dependent mechanical behavior known as viscoelasticity. Viscoelasticity affects the relationship between relative resistance change and mechanical strain over time and becomes particularly important over long times, as well as at high temperatures, radiation aging, and in the presence of high mechanical stresses. The characterization of these influences on the piezoresistivity is therefore crucial to maintain sensor stability under harsh conditions. While experimental studies have been conducted, the effect of viscoelasticity has not been thorougly analyzed. Here we study resistance relaxation and hysteresis in carbon nanotube nanocomposites under the influence of matrix viscoelasticity. First we characterize the multiaxial viscoelastic behavior directly without invoking the common assumption of constant Poisson’s ratio, which is central to predicting resistnace relaxation and hysteresis. We then apply the viscoelastic master curve to a micromechanics-based numerical piezoresistive model and study the relative resistance change under relaxation, creep, and deformation-controlled cyclic loading. The results are compared to the common assumptions of constant Poisson’s ratio and constant bulk modulus. We show that the time-dependent transverse deformation leads to network compaction and is the mechanism behind resistance relaxation and hysteresis.},
   author = {Wolfgang Klimm and Kawai Kwok},
   doi = {10.1007/S10999-022-09603-Y},
   isbn = {0123456789},
   issn = {1573-8841},
   journal = {International Journal of Mechanics and Materials in Design 2022},
   keywords = {Characterization and Evaluation of Materials,Classical Mechanics,Engineering Design,Solid Mechanics},
   month = {8},
   pages = {1-15},
   publisher = {Springer},
   title = {Mechanism of resistance relaxation and hysteresis in viscoelastic piezoresistive polymer nanocomposites},
   url = {https://link.springer.com/article/10.1007/s10999-022-09603-y},
   year = {2022},
}
@article{Su2022,
   abstract = {<p>In this study, highly-sensitive piezoresistive strain sensors based on gold nanoparticle thin films deposited on a stretchable PDMS substrate by centrifugation were developed to measure arterial pulse waveform. By controlling carbon chain length of surfactants, pH value and particle density of the colloidal solutions, the gauge factors of nanoparticle thin film sensors can be optimized up to 677 in tensile mode and 338 in compressive mode, and the pressure sensitivity up to 350. Low pH and thin nanoparticle films produce positive influences to superior gauge factors. It has been demonstrated that nanoparticle thin film sensors on PDMS substrates were successfully applied to sense arterial pulses in different body positions, including wrist, elbow crease, neck, and chest.</p>},
