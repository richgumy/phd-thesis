@article{Devaraj2018,
	title        = {{The development of highly flexible stretch sensors for a Robotic Hand}},
	author       = {Harish Devaraj and Tim Giffney and Adeline Petit and Mahtab Assadian and Kean Aw},
	year         = 2018,
	month        = sep,
	journal      = {Robotics},
	publisher    = {{Mdpi Ag}},
	volume       = 7,
	doi          = {10.3390/robotics7030054},
	issn         = 22186581,
	abstract     = {Demand for highly compliant mechanical sensors for use in the fields of robotics and wearable electronics has been constantly rising in recent times. Carbon based materials, and especially, carbon nanotubes, have been widely studied as a candidate piezoresistive sensing medium in these devices due to their favorable structural morphology. In this paper three different carbon based materials, namely carbon black, graphene nano-platelets, and multi-walled carbon nanotubes, were utilized as large stretch sensors capable of measuring stretches over 250%. These stretch sensors can be used in robotic hands/arms to determine the angular position of joints. Analysis was also carried out to understand the effect of the morphologies of the carbon particles on the electromechanical response of the sensors. Sensors with gauge factors ranging from one to 1.75 for strain up to 200% were obtained. Among these sensors, the stretch sensors with carbon black/silicone composite were found to have the highest gauge factor while demonstrating acceptable hysteresis in most robotic hand applications. The highly flexible stretch sensors demonstrated in this work show high levels of compliance and conformance making them ideal candidates as sensors for soft robotics.},
	issue        = 3,
	keywords     = {Carbon nanoparticle,Flexible sensor,Large stretch sensor,Piezoresistive,Robotic exoskeleton}
}
@article{Giffney2017,
	title        = {{Highly stretchable printed strain sensors using multi-walled carbon nanotube/silicone rubber composites}},
	author       = {Tim Giffney and Estelle Bejanin and Agee S. Kurian and Jadranka Travas-Sejdic and Kean Aw},
	year         = 2017,
	month        = jun,
	journal      = {Sensors and Actuators, A: Physical},
	publisher    = {{Elsevier B.V.}},
	volume       = 259,
	pages        = {44--49},
	doi          = {10.1016/j.sna.2017.03.005},
	issn         = {09244247},
	abstract     = {The developing fields of wearable electronics and soft robotics have created a strong demand for flexible and stretchable strain sensors. This work presents a printing technique and conductive multiwalled carbon nanotube/silicone polymer nanocomposites applied to achieve embedded piezoresistive large strain sensors. A printing process that allows the sensors to be produced inside a silicone substrate has compelling advantages: fabricated devices are well insulated and protected against wear. These process features simplify fabrication of complex patterns. Printed sensors can withstand stretching of up to 300%, with maximum hysteresis of 11% for 300% maximum strain, and gauge factors between 1.0 and 1.5 depending on the selected composition. The devices developed in this work have potential applications in the measurement of human body motion and position control of soft actuators.},
	keywords     = {Carbon nanotube,Printing,Strain sensor}
}
@inproceedings{Mouri2019,
	title        = {{Development of new flexible dry electrode for myoelectric sensor using conductive silicone}},
	author       = {Yasuhiro Mouri and Yuta Murai and Yoshiko Yabuki and Takumi Kato and Hideki Ohmae and Yoshihiro Tomita and Shunta Togo and Yinlai Jiang and Hiroshi Yokoi},
	year         = 2019,
	month        = jan,
	journal      = {2018 IEEE International Conference on Cyborg and Bionic Systems, CBS 2018},
	publisher    = {{Institute of Electrical and Electronics Engineers Inc.}},
	pages        = {478--482},
	doi          = {10.1109/cbs.2018.8612280},
	isbn         = 9781538673553,
	url          = {https://ieeexplore-ieee-org/document/8612280},
	abstract     = {This paper reports the development of a new electromyography (EMG) sensor for the myoelectric prosthetic hand. The myoelectric prosthetic hand is controlled by EMG which is detected from the surface of human skin. Our study focuses on the dry type sensor with flexible adhesion material to fit to the skin surface instead of metal electrodes. The developed electrode consisted of silicone and conductive silicone blended with conductive carbon black. These electrodes were mounted on myoelectric prosthetic hands that were applied to perform the Pick and Place Tasks to test the total performance of the control system. The results showed that the developed material obtained a thinner sensor than the conventional ones, which enabled it to naturally trace to the subject's forearm shape without burden on the skin.}
