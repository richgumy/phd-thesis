@Article{Kodaira2019,
  author   = {A. {Kodaira} and K. {Asaka} and T. {Horiuchi} and G. {Endo} and H. {Nabae} and K. {Suzumori}},
  journal  = {IEEE Robotics and Automation Letters},
  title    = {IPMC Monolithic Thin Film Robots Fabricated Through a Multi-Layer Casting Process},
  year     = {2019},
  issn     = {2377-3766},
  month    = apr,
  number   = {2},
  pages    = {1335--1342},
  volume   = {4},
  abstract = {In this letter, we report on a multi-layer casting process of an ionic polymer metal composite actuator to produce a monolithic thin film robot (MTFR). This robot is developed on the basis of the novel concept of a soft robot, which consists of a body united with actuators and does not require mechanical and electrical assembly process. An MTFR is made from only one thin film that involves membranes of a variety of thicknesses. The thick parts of the membrane function as bone structures, and the thin parts as actuators. This integral multi-function structure of the MTFR realizes an assembly-less manufacturing process. To produce the monolithic film with a distributed thickness,we propose the use of a multi-layer casting process. To demonstrate the potential of an MTFR for use in a biomimetic application, we manufactured butterfly- and anomalocaris-like MTFRs. Both the robots can be controlled by applying periodic voltage signals from only one electrode, and each actuator can be driven separately by utilizing the resonance phenomena.},
  doi      = {10.1109/LRA.2019.2895398},
  keywords = {biomimetics;bone;casting;composite materials;electric actuators;electroactive polymer actuators;electrodes;intelligent materials;membranes;polymers;IPMC monolithic;multilayer casting process;ionic polymer metal composite actuator;MTFR;soft robot;mechanical assembly process;electrical assembly process;integral multifunction structure;assembly-less manufacturing process;monolithic film;Casting;Actuators;Dispersion;Soft robotics;Gold;Electrodes;Soft sensors and actuators;soft robot applications;flexible robots;IPMC actuator},
}

@Article{Hubbard2014,
  author   = {J. J. {Hubbard} and M. {Fleming} and V. {Palmre} and D. {Pugal} and K. J. {Kim} and K. K. {Leang}},
  journal  = {IEEE Journal of Oceanic Engineering},
  title    = {Monolithic IPMC Fins for Propulsion and Maneuvering in Bioinspired Underwater Robotics},
  year     = {2014},
  issn     = {1558-1691},
  month    = jul,
  number   = {3},
  pages    = {540--551},
  volume   = {39},
  abstract = {Emerging bioinspired underwater systems, such as autonomous ocean mapping and surveillance vehicles, that maneuver through their environment by mimicking the swimming motion of aquatic animals, can benefit from soft monolithic actuators and control surfaces capable of undergoing complex deformations. Herein, an electrically driven ionic polymer-metal composite (IPMC) artificial muscle with uniquely patterned electrodes for creating complex deformations is presented. The surface electrode pattern on the IPMC is created using a simple surface machining process. By selectively activating specific regions of the IPMC, bending, twisting, flapping, and other bioinspired locomotive behavior can be achieved. For instance, the bending and twisting response of an example 50 mm × 25 mm × 0.5 mm patterned IPMC actuator is characterized to determine its range of motion, output force and torque, as well as its effectiveness as a fish-fin-like propulsor. The experimental results show that the twisting angle exceeds 8deg; the blocking tip force and torque can be as high as 16.5 mN (at 3 V) and 0.83 N·mm (at 4 V), respectively (driven at 0.05 Hz); and an average thrust force of approximately 0.4 mN (driven by 4-V sinusoidal input at 1 Hz) can be generated. These newly developed IPMC fins can be exploited to create novel and efficient propulsors for next-generation underwater robotic vehicles. An example bioinspired robotic fish is presented which exploits the capabilities of the patterned IPMCs for propulsion and maneuvering, where an average maximum swimming speed of approximately 28 mm/s is reported.},
  doi      = {10.1109/JOE.2013.2259318},
  keywords = {machining;marine propulsion;mobile robots;underwater vehicles;monolithic IPMC fins;maneuvering;bioinspired underwater robotics;bioinspired underwater systems;autonomous ocean mapping;surveillance vehicles;swimming motion;aquatic animals;soft monolithic actuators;electrically driven ionic polymer-metal composite;IPMC artificial muscle;complex deformations;surface electrode pattern;surface machining process;activating specific regions;bioinspired locomotive behavior;IPMC actuator;fish-fin-like propulsor;blocking tip force;torque;next-generation underwater robotic vehicles;bioinspired robotic fish;average maximum swimming speed;Robots;Electrodes;Gold;Actuators;Surface treatment;Force;Propulsion;Ionic polymer metal composite actuators;propulsion and maneuvering;UAVs;underwater robotics},
}

@InProceedings{Khawwaf2019,
  author    = {J. {Khawwaf} and J. {Zheng} and H. {Wang} and Z. {Man}},
  booktitle = {2019 Chinese Control Conference (CCC)},
  title     = {Practical Model-Free Robust Control Design for an Underwater IPMC Actuator},
  year      = {2019},
  month     = jul,
  pages     = {3214--3219},
  abstract  = {In a trend of the recent development of smart actuators, ionic polymer metal composite (IPMC) is considered as one of the new and innovative smart materials with superior advantages. The IPMC is suited to be utilized in medical micro robots and bio-inspired aquatic robotics. It is distinguished by nimble, soft, silent, flexible and lightweight properties. Furthermore, the driving voltage, which is being consumed, is fairly low, while the generated deflection is sufficiently large. In fact, for the IPMC actuator, hysteresis and creep properties are inevitable; and it is a great challenge to handle them and to achieve high-precision tracking in control design, especially when an internal morphology of system is complex and not fully understood. Therefore, this paper proposes a new model-free control approach for an underwater IPMC actuator to overcome the lack of its exact model and achieve an accurate micro-tracking trajectory. This approach is synthesized based on a nonlinear extended state observer (NESO) technique and a sliding mode controller (SMC), is added as an extra input to deal with the estimation error and drive the system. Finally, the proposed control is experimentally verified to show its benefits in comparison with a nonsingular terminal sliding mode (NTSM) controller. The experimental results indicate that the proposed controller is capable of delivering high-precision tracking and achieve high robustness.},
  doi       = {10.23919/ChiCC.2019.8866467},
  issn      = {1934-1768},
  keywords  = {actuators;control system synthesis;electroactive polymer actuators;intelligent actuators;intelligent materials;microrobots;mobile robots;nonlinear control systems;observers;robot dynamics;robust control;underwater vehicles;variable structure systems;sliding mode controller;high-precision tracking;practical model-free robust control design;underwater IPMC actuator;smart actuators;ionic polymer metal composite;new materials;innovative smart materials;superior advantages;bio-inspired aquatic robotics;nimble properties;soft properties;silent properties;flexible properties;lightweight properties;driving voltage;model-free control approach;exact model;accurate microtracking trajectory;nonlinear extended state observer technique;Actuators;Uncertainty;Mathematical model;Adaptation models;Control design;Observers;Estimation error;IPMC;nonlinear extended state observer;sliding mode control},
}

@Article{Chang2013,
  author   = {Y. {Chang} and W. {Kim}},
  journal  = {IEEE/ASME Transactions on Mechatronics},
  title    = {Aquatic Ionic-Polymer-Metal-Composite Insectile Robot With Multi-DOF Legs},
  year     = {2013},
  issn     = {1941-014X},
  month    = apr,
  number   = {2},
  pages    = {547--555},
  volume   = {18},
  abstract = {Ionic polymer-metal composite (IPMC) is used in various bioinspired systems, such as fish- and tadpole-like robots swimming in water. The deflection of this smart material results from several external factors, such as water distribution and concentration. IPMC strips with a variety of water concentration on the surfaces and the surface conductivity show various deflection patterns. In this paper, IPMC strips in four initial wetness conditions (100\%, 50\%, 0\% wet on the anode surfaces but 100\% wet on the cathode surfaces, and the last one submerged in deionized water) were tested. Even without any external excitation, the strips can bend due to nonuniform water distribution. In order to understand the effects of surface conductivity in an aquatic environment, an IPMC strip with two wires connected to two distinct spots was used to demonstrate the power loss due to the surface resistance. Three types of input signals, sawtooth, sinusoidal, and square waves, were used to compare the difference between the input and output signals measured at the two spots. Thick (1 mm) IPMC strips were fabricated and employed in this study to sustain and drive the robot with sufficient forces. Finally, an aquatic walking robot (102 × 80 × 43 mm, 39 g) with six 2-degree-of-freedom (2-DOF) legs has been designed, implemented, and walked in water at the speed of 0.5 mm/s. The average power consumption is 8 W per leg. Each leg has a thigh and a shank to generate 2-DOF motions. Each set of three legs walked together as a tripod to maintain the stability in operation.},
  doi      = {10.1109/TMECH.2012.2210904},
  keywords = {electroactive polymer actuators;intelligent materials;legged locomotion;marine vehicles;signal processing;aquatic ionic-polymer-metal-composite insectile robot;multiDOF legs;bioinspired systems;fish-like robots;tadpole-like robots;smart material;water distribution;water concentration;IPMC strips;surface conductivity;sawtooth waves;sinusoidal waves;square waves;aquatic walking robot;power 8 W;Strips;Legged locomotion;Anodes;Metals;Surface treatment;Aquatic walking robot;biomimetic robot;ionic-polymer-metal composite (IPMC);2-degree-of-freedom (DOF) leg},
}

@Article{Kellaris2018,
  author   = {Kellaris, Nicholas and Gopaluni Venkata, Vidyacharan and Smith, Garrett M. and Mitchell, Shane K. and Keplinger, Christoph},
  journal  = {Sci. Robotics},
  title    = {Peano-HASEL actuators: Muscle-mimetic, electrohydraulic transducers that linearly contract on activation},
  year     = {2018},
  month    = jan,
  number   = {14},
  pages    = {eaar3276},
  volume   = {3},
  abstract = {Soft robotic systems are well suited to unstructured, dynamic tasks and environments, owing to their ability to adapt and conform without damaging themselves or their surroundings. These abilities are crucial in areas such as human-robot interaction. Soft robotic systems are currently limited by the soft actuators that power them. To date, most soft actuators are based on pneumatics or shape-memory alloys, which have issues with efficiency, response speed, and portability. Dielectric elastomer actuators (DEAs) are controlled and powered electrically and excel with muscle-like actuation, but they typically require a rigid frame and prestretch to perform effectively. In addition, DEAs require complex stacks or structures to achieve linear contraction modes. We present a class of soft electrohydraulic transducers, termed Peano-HASEL (hydraulically amplified self-healing electrostatic) actuators, that combine the strengths of fluidic actuators and electrostatic actuators, while addressing many of their issues. These actuators use both electrostatic and hydraulic principles to linearly contract on application of voltage in a muscle-like fashion, without rigid frames, prestretch, or stacked configurations. We fabricated these actuators using a facile heat-sealing method with inexpensive commercially available materials. These prototypical devices demonstrated controllable linear contraction up to 10\%, a strain rate of 900\% per second, actuation at 50 hertz, and the ability to lift more than 200 times their weight. In addition, these actuators featured characteristics such as high optical transparency and the ability to self-sense their deformation state. Hence, this class of actuators demonstrates promise for applications such as active prostheses, medical and industrial automation, and autonomous robotic devices.},
  url      = {http://robotics.sciencemag.org/content/3/14/eaar3276.abstract},
}

