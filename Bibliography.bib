@article{Liu2022,
	title        = {{Investigation of stress relaxation in SAC305 with micro-Raman spectroscopy}},
	author       = {E. Liu and Sri Krishna Bhogaraju and Bernhard Wunderle and Gordon Elger},
	year         = 2022,
	month        = nov,
	journal      = {Microelectronics Reliability},
	publisher    = {{Pergamon}},
