   publisher = {World Scientific Publishing Co. Pte Ltd},
   title = {Viscoelastic properties of silicone-based magnetorheological elastomers},
   volume = {21},
   year = {2007},
}
@article{Jolly1996,
   abstract = {The mechanical response of elastomer composites to applied magnetic fields is examined. These elastomer composites consist of carbonyl iron particles embedded within a molded elastomer matrix. The composite is subjected to a strong magnetic field during curing, which causes the iron particles to form columnar structures that are parallel to the applied field. This special composite geometry is known to enhance the mechanical response to the application of post-cured magnetic fields. Experimental data is presented that shows that up to a 0.6 MPa change in mechanical shear modulus (which represents 30-40% change in modulus for the materials tested) is possible in response to an applied magnetic field for a composite containing 30% (V/V) iron particles. A simple quasi-static dipole model is presented to examine the magnetoviscoelastic effect of these elastomer composites. The model is semi-empirical in that it may be fit to experimental data over a broad range of applied fields by adjusting a parameter that accounts for unmodeled multipolar magnetic interactions between particles within the composite. Such elastomer composites hold promise in enabling variable stiffness devices and adaptive structures.},
   author = {Mark R Jolly and J David Carlson and Beth C Munoz and Todd A Bullions},
   doi = {10.1177/1045389x9600700601},
   issn = {1045-389X},
   issue = {6},
