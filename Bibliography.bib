	pages        = {787--793},
	doi          = {10.1177/104538903039260},
	issn         = {1045389x},
	url          = {https://journals.sagepub.com/doi/10.1177/104538903039260},
	abstract     = {Polyacrylate dielectric elastomers have yielded extremely large strain and elastic energy density suggesting that they are useful for many actuator applications. A thorough understanding of the phy...},
	issue        = 12,
	keywords     = {actuator,adhesive,breakdown,dielectric,elastomer,electric,electrostriction,strain}
