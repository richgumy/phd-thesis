   title = {Multiscale Numerical Modeling for Prediction of Piezoresistive Effect for Polymer Composites with a Highly Segregated Structure},
   volume = {11},
   url = {/pmc/articles/PMC7827821/ /pmc/articles/PMC7827821/?report=abstract https://www.ncbi.nlm.nih.gov/pmc/articles/PMC7827821/},
