@article{Tiwana2012,
   abstract = {Any device which senses information such as shape, texture, softness, temperature, vibration or shear and normal forces, by physical contact or touch, can be termed a tactile sensor. The importance of tactile sensor technology was recognized in the 1980s, along with a realization of the importance of computers and robotics. Despite this awareness, tactile sensors failed to be strongly adopted in industrial or consumer markets. In this paper, previous expectations of tactile sensors have been reviewed and the reasons for their failure to meet these expectations are discussed. The evolution of different tactile transduction principles, state of art designs and fabrication methods, and their pros and cons, are analyzed. From current development trends, new application areas for tactile sensors have been proposed. Literature from the last few decades has been revisited, and areas which are not appropriate for the use of tactile sensors have been identified. Similarly, the challenges that this technology needs to overcome in order to find its place in the market have been highlighted. © 2012 Published by Elsevier B.V.},
   author = {Mohsin I. Tiwana and Stephen J. Redmond and Nigel H. Lovell},
   doi = {10.1016/J.SNA.2012.02.051},
   issn = {0924-4247},
   journal = {Sensors and Actuators A: Physical},
   keywords = {Advancements and challenges,Review of technology,Shear-stress sensors,Tactile devices,Tactile sensing},
   month = {6},
   pages = {17-31},
   publisher = {Elsevier},
   title = {A review of tactile sensing technologies with applications in biomedical engineering},
   volume = {179},
   year = {2012},
}
@article{Yuan2023,
   abstract = {With the emphasis on healthcare, early childhood education, and fitness, non-invasive measurement and recognition methods have received more attention. Pressure sensing has been extensively studied due to its advantages of simple structure, easy access, visualization application, and harmlessness. This paper introduces a smart pressure e-mat (SPeM) system based on a piezoresistive material Velostat for human monitoring applications, including sleeping postures, sports, and yoga recognition. After a subsystem scans e-mat readings and processes the signal, it generates a pressure image stream. Deep neural networks (DNNs) are used to fit and train the pressure image stream and recognize the corresponding human behavior. Four sleeping postures and five dynamic activities inspired by Nintendo Switch Ring Fit Adventure (RFA) are used as a preliminary validation of the proposed SPeM system. The SPeM system achieves high accuracies on both applications, which demonstrates the high accuracy and generalization ability of the models. Compared with other pressure sensor-based systems, SPeM possesses more flexible applications and commercial application prospects, with reliable, robust, and repeatable properties.},
   author = {Liangqi Yuan and Student Member and Yuan Wei and Jia Li and Senior Member},
   journal = {arxiv},
   keywords = {Index Terms-Pressure sensor,activity recog-nition,deep learning,healthcare,human sensing},
   month = {5},
   title = {Smart Pressure e-Mat for Human Sleeping Posture and Dynamic Activity Recognition},
   url = {https://arxiv.org/abs/2305.11367v1},
   year = {2023},
}
@article{Shang2016,
   abstract = {A series of flexible composites with a carbon black (CB) filled silicone rubber matrix were made by an improved process in this work. A low percolation threshold with a mass ratio of 2.99% CB was achieved. The piezoresistive behavior of CB/silicone composites above the critical value, with the mass ratio of carbon black to the silicone rubber ranging from 0.01 to 0.2, was studied. The piezoresistive behavior was different from each other for the composites with different CB contents. But, the composites show an excellent repeatability of piezoresistivity under cyclic compression, no matter with low filler content or with high filler content. The most interesting phenomena were that the plots of gauge factor versus strain of the composites with different CB contents constructed a master curve and the curve could be well fitted by a function. It was showed that the gauge factor of the composites was strain-controlled showing a promising prospect of application.},
   author = {Shuying Shang and Yujuan Yue and Xiaoer Wang},
   doi = {10.1063/1.4973274/916719},
   issn = {10897623},
   issue = {12},
   journal = {Review of Scientific Instruments},
   month = {12},
   pages = {123910},
   publisher = {American Institute of Physics Inc.},
   title = {Piezoresistive strain sensing of carbon black /silicone composites above percolation threshold},
   volume = {87},
   url = {/aip/rsi/article/87/12/123910/916719/Piezoresistive-strain-sensing-of-carbon-black},
   year = {2016},
}
